unit UnUSUARIOSTypes;

interface

type
  USUARIOS = class
  const
    NomeTabela = 'USUARIOS';
  ID NUMERIC(18)PRIMARY KEY UNIQUE NOT NULL, NOME VARCHAR(50)NOT NULL,
    SENHA VARCHAR(50)NOT NULL, IMAGEMPLANOFUNDO BLOB, EMAIL VARCHAR(40)UNIQUE,
    EMPRESA VARCHAR(40), USUARIO VARCHAR(50)UNIQUE, SETOR VARCHAR(40),
    CARGO VARCHAR(40), TIPOPERMICAO

  end;

implementation

end.
