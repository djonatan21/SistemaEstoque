unit UnREGISTRARVENDATypes;

interface

type
  REGISTRARVENDA = class
    ITEM = '';
    CODIGO_PRODUTO NUMERIC(5, 9)NOT NULL, VALOR NUMERIC(10, 2)NOT NULL,
      QUANTIDADE NUMERIC(18, 3)NOT NULL, VALOR_TOTAL NUMERIC(18, 2),
      UNIDADE VARCHAR(10)NOT NULL, CODIGO_BARRA VARCHAR(20), DESCRICAO
  end;

implementation

end.
